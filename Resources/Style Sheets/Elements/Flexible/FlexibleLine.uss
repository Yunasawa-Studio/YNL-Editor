#Horizontal
{
	background-color: #505050;

	height: 1.5px;

	border-width: 0px;
	border-bottom-width: 2px;

	border-color: transparent;
	border-bottom-color: #272727;

	margin-top: 5px;
	margin-left: 15px;
	margin-right: 15px;
}

#Vertical
{
	background-color: #505050;

	width: 1.5px;
	max-height: 100%;

	border-width: 0px;
	border-right-width: 2px;

	border-color: transparent;
	border-right-color: #272727;

	margin-top: 5px;
	margin-bottom: 5px;
}